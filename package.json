{
  "name": "@sayafi/utils",
  "version": "1.0.25",
  "author": "sayafiMustafa",
  "homepage": "https://github.com/sayafiMustafa/my-utils",
  "type": "module",
  "keywords": [
    "utils"
  ],
  "exports": {
    "./browser": {
      "types": "./browser/index.d.ts",
      "import": "./browser/index.js",
      "require": "./browser/index.cjs"
    },
    "./node": {
      "types": "./node/index.d.ts",
      "import": "./node/index.js",
      "require": "./node/index.cjs"
    },
    "./node/inquirer": {
      "types": "./node/inquirer/index.d.ts",
      "import": "./node/inquirer/index.js"
    }
  },
  "files": [
    "browser",
    "node"
  ],
  "scripts": {
    "build": "rollup --config",
    "develop": "npm run build -- --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@jeniex/eslint-config": "^1.0.2",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.5.7",
    "@violentmonkey/types": "^0.1.5",
    "inquirer": "^9.2.11",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "tslib": "^2.6.2",
    "tsx": "^3.12.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
